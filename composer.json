{
  "name": "enea/laravel-cashier",
  "description": "Simple package for product management",
  "keywords": [
    "cashier",
    "sales",
    "cart",
    "shopping",
    "manager"
  ],
  "authors": [
    {
      "name": "enea dhack",
      "email": "eneas.so@live.com"
    }
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^8.0||^9.0",
    "vaened/php-enum": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3.7",
    "orchestra/testbench": "^6.0||^7.0"
  },
  "autoload": {
    "psr-4": {
      "Enea\\Cashier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Enea\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Enea\\Cashier\\CashierServiceProvider"
      ]
    }
  }
}
